<h1>Questions: </h1>

<% if @questions %>
  <ul>
    <% @questions.each do |question| %>
      <li>
        <%= link_to "#{question.title}", question_path(question) %>
      </li>
    <% end %>
  </ul>
<% end %>

<button class="button"> <%= link_to "Add New Question", new_question_path %> </button>
